    "steps": [
        {
            "name": "get_user_details",
            "url": "/users/octocat",
            "method": "GET",
            "request": {
            },
            "assertions": {
                "status": 200,
                "body": {
                    "created_at" : "$LOCAL.DATETIME.BEFORE:2015-09-14T09:49:34.000Z",
                    "updated_at" : "$LOCAL.DATETIME.AFTER:2015-09-14T09:49:34.000Z"
                }
            }
        }
    ]
}